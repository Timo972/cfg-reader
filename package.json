  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Timo972/cfg-reader/issues"
  },
  "homepage": "https://github.com/Timo972/cfg-reader#readme",
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.2.5",
    "@types/node": "^16.10.2",
    "fs": "0.0.1-security",
    "mocha": "^8.3.0",
    "rollup": "^2.58.0"
  },
  "engines": {
    "node": ">= 12"
  },
  "gypfile": true
